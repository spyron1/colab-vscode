{
  "name": "colab",
  "publisher": "google",
  "displayName": "Colab",
  "description": "Connect to Colab runtimes.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Notebooks"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "colab.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "clean": "rm -rf out/*",
    "compile": "tsc -p ./",
    "lint": "eslint src",
    "pretest": "npm run compile && npm run lint",
    "test:xvfb": "xvfb-run -s '-screen 0 1024x768x24' vscode-test",
    "test": "if [ -z \"$DISPLAY\" ]; then npm run test:xvfb; else vscode-test; fi",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "mocha": "^10.2.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "glob": "^11.0.0"
  }
}